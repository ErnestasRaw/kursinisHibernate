<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="640.0" prefWidth="850.0" style="-fx-background-color: #1955d8" xmlns="http://javafx.com/javafx/20.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.kursinis.kursinis_hibernate.fxControllers.WarehouseController">
    <ListView fx:id="warehouseList" layoutX="20.0" layoutY="14.0" onMouseClicked="#onWarehouseSelect" prefHeight="293.0"
              prefWidth="210.0"/>
    <Button layoutX="425.0" layoutY="14.0" mnemonicParsing="false" onAction="#addNewWarehouse" prefHeight="26.0"
            prefWidth="86.0" text="Add new"/>
    <Button layoutX="528.0" layoutY="14.0" mnemonicParsing="false" onAction="#updateWarehouse" prefHeight="26.0"
            prefWidth="83.0" text="Update"/>
    <Button layoutX="629.0" layoutY="14.0" mnemonicParsing="false" onAction="#removeWarehouse" prefHeight="26.0"
            prefWidth="97.0" text="Remove"/>
    <TextField fx:id="addressWarehouseField" layoutX="260.0" layoutY="51.0" promptText="Address"/>
    <TextField fx:id="titleWarehouseField" layoutX="260.0" layoutY="14.0" promptText="Title"/>
</Pane>
