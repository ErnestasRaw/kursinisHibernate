    <TableView fx:id="cartTable" layoutX="436.0" layoutY="51.0" prefHeight="520.0" prefWidth="400.0">
        <columns>
            <TableColumn fx:id="cartTitleColumn" prefWidth="76.0" text="Pavadinimas"/>
