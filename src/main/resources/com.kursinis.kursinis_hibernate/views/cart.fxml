    <Label layoutX="14.0" layoutY="14.0" text="Produktai" textFill="WHITE">
        <font>
            <Font size="26.0"/>
        </font>
    </Label>
    <Label layoutX="436.0" layoutY="14.0" text="Krepšelis" textFill="WHITE">
        <font>
            <Font size="26.0"/>
        </font>
    </Label>
    <Text fill="WHITE" layoutX="74.0" layoutY="607.0" strokeType="OUTSIDE" strokeWidth="0.0"
